 column=(type=datetime updatewhereclause=yes name=created_date dbname="receiving_bol.created_date" )
 column=(type=char(40) updatewhereclause=yes name=bol dbname="inbound_coil_status.bol" )
 column=(type=char(32) updatewhereclause=yes name=coil_number dbname="inbound_coil_status.coil_number" )
 column=(type=decimal(0) updatewhereclause=yes name=coil_abc_num dbname="receiving_bol_coil.coil_abc_num" )
 column=(type=decimal(0) updatewhereclause=yes name=edi_file_id dbname="inbound_coil_status.edi_file_id" )
 retrieve="select distinct 
			receiving_bol.customer_id,
			customer.customer_short_name,
			receiving_bol.created_date,
			inbound_coil_status.bol,
			inbound_coil_status.coil_number,
			receiving_bol_coil.coil_abc_num,
			inbound_coil_status.edi_file_id
from   	receiving_bol_coil
       	join receiving_bol on receiving_bol_coil.receiving_bol_id = receiving_bol.receiving_bol_id
       	join inbound_coil_status on inbound_coil_status.bol = receiving_bol.bol
			join customer on customer.customer_id = receiving_bol.customer_id
where  	receiving_bol.customer_id = :al_customer_id
and    	inbound_coil_status.bol = :as_bol
and      inbound_coil_status.coil_number = :as_coil_number
and      receiving_bol_coil.coil_abc_num = :al_coil_abc_num
order by	receiving_bol.customer_id, receiving_bol.created_date, inbound_coil_status.bol" arguments=(("al_customer_id", number),("as_bol", string),("as_coil_number", string),("al_coil_abc_num", number)) )
