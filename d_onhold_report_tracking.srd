 column=(type=char(18) updatewhereclause=yes name=customer_name dbname="customer_name" )
 column=(type=char(17) updatewhereclause=yes name=sheet_skid_num dbname="sheet_skid_num" )
 column=(type=char(40) updatewhereclause=yes name=coil_abc_num dbname="coil_abc_num" )
 column=(type=char(32) updatewhereclause=yes name=coil_org_num dbname="coil_org_num" )
 column=(type=datetime updatewhereclause=yes name=transaction_date_time dbname="coil_track.transaction_date_time" )
 column=(type=char(18) updatewhereclause=yes name=enduser_name dbname="enduser_name" )
 column=(type=char(22) updatewhereclause=yes name=enduser_part_num dbname="enduser_part_num" )
 column=(type=char(40) updatewhereclause=yes name=order_item_desc dbname="order_item_desc" )
 column=(type=decimal(0) updatewhereclause=yes name=net_wt dbname="coil.net_wt" )
 column=(type=char(11) updatewhereclause=yes name=skid_pieces dbname="skid_pieces" )
 column=(type=char(11) updatewhereclause=yes name=status dbname="status" )
