 column=(type=char(11) updatewhereclause=yes name=processed_t dbname="processed_t" )
 column=(type=char(11) updatewhereclause=yes name=scrap_net_t dbname="scrap_net_t" )
 column=(type=char(18) updatewhereclause=yes name=customer_order_scrap_handing_type dbname="customer_order.scrap_handing_type" )
 column=(type=char(18) updatewhereclause=yes name=order_item_sheet_type dbname="order_item.sheet_type" )
 column=(type=char(32) updatewhereclause=yes name=ab_die_id dbname="ab_die_id" )
 retrieve="  select ab_job.ab_job_num, 
         line.line_desc,
         ab_job.Job_done_time,  
         ltrim(rtrim(c1.customer_short_name)) || ' (' || ltrim(rtrim(c2.customer_short_name)) || ')' customer_name,
         order_item.material_end_use,
         order_item.alloy2,
         order_item.temper,   
         order_item.gauge,
         (  select   min(coil.coil_width)
            from     process_coil 
                     join coil on coil.coil_abc_num = process_coil.coil_abc_num
            where    process_coil.ab_job_num = ab_job.ab_job_num
         ) coil_width,
         '           ' processed_t,
         '           ' scrap_net_t,
			customer_order.scrap_handing_type,
         order_item.sheet_type,
         (select die_name from die where die_id = ab_job.die_id) ab_die_id         
    from ab_job
         join order_item on order_item.order_abc_num = ab_job.order_abc_num and order_item.order_item_num = ab_job.order_item_num
         join customer_order on customer_order.order_abc_num = order_item.order_abc_num 
         join customer c1 on c1.customer_id = customer_order.orig_customer_id 
         join customer c2 on c2.customer_id = customer_order.enduser_id
         left outer join line on line.line_num = ab_job.line_num
         left outer join sketch_jpg on sketch_jpg.sketch_id = ab_job.sketch_id
         left outer join metal_density on metal_density.metal_alloy = order_item.alloy2
  where  ab_job.job_status = 0" )
