 retrieve="  SELECT ~"SHEET_SKID~".~"SHEET_SKID_DISPLAY_NUM~",
			~"AB_JOB~".~"AB_JOB_NUM~",
      (select customer_short_name from customer where customer_id = customer_order.orig_customer_id) customer_name, /* Alex Gerlants. 05/03/2016 */ 
      (select customer_short_name from customer where customer_id = customer_order.enduser_id) enduser_name, /* Alex Gerlants. 05/03/2016 */
      ~"ORDER_ITEM~".~"ENDUSER_PART_NUM~",
      ~"SHEET_SKID~".~"SKID_PIECES~",
      ~"SHEET_SKID~".~"SHEET_NET_WT~",
      (select skid_status_desc from skid_status where skid_status_code = sheet_skid.skid_sheet_status) skid_status_desc, /* Alex Gerlants. 05/03/2016 */ 
      ~"COIL~".~"COIL_ORG_NUM~",  
      NULL ~"SPEC_T~",
      f_get_material(~"COIL~".~"COIL_ALLOY2~", ~"COIL~".~"COIL_TEMPER~") material,
      ~"COIL~".~"LOT_NUM~",
      ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_PIECES~",
      ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_NET_WT~", 
      f_get_skid_duration(sheet_skid.sheet_skid_num) duration,
         
         ~"SHEET_SKID~".~"SHEET_SKID_NUM~",   
         ~"SHEET_SKID~".~"SKID_DATE~",
         ~"PRODUCTION_SHEET_ITEM~".~"COIL_ABC_NUM~",   
         ~"PRODUCTION_SHEET_ITEM~".~"AB_JOB_NUM~",   
      ~"SHEET_SKID_DETAIL~".~"PROD_ITEM_NUM~"
    FROM ~"AB_JOB~",   
         ~"CUSTOMER_ORDER~",   
         ~"SHEET_SKID~",   
         ~"SHEET_SKID_DETAIL~",   
         ~"PRODUCTION_SHEET_ITEM~",   
         ~"ORDER_ITEM~",   
         ~"COIL~"
   WHERE ( ~"AB_JOB~".~"ORDER_ABC_NUM~" = ~"CUSTOMER_ORDER~".~"ORDER_ABC_NUM~" ) and  
         ( ~"AB_JOB~".~"AB_JOB_NUM~" = ~"SHEET_SKID~".~"AB_JOB_NUM~" ) and  
         ( ~"SHEET_SKID_DETAIL~".~"SHEET_SKID_NUM~" = ~"SHEET_SKID~".~"SHEET_SKID_NUM~" ) and  
         ( ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_NUM~" = ~"SHEET_SKID_DETAIL~".~"PROD_ITEM_NUM~" ) and  
         ( ~"AB_JOB~".~"ORDER_ABC_NUM~" = ~"ORDER_ITEM~".~"ORDER_ABC_NUM~" ) and  
         ( ~"AB_JOB~".~"ORDER_ITEM_NUM~" = ~"ORDER_ITEM~".~"ORDER_ITEM_NUM~" ) and  
         ( ~"PRODUCTION_SHEET_ITEM~".~"COIL_ABC_NUM~" = ~"COIL~".~"COIL_ABC_NUM~" ) and 
	      ( ~"SHEET_SKID~".~"SKID_SHEET_STATUS~" <> 0 ) and
   	   ( ~"SHEET_SKID~".~"SKID_SHEET_STATUS~" <> 6 ) 
"  sort="sheet_skid_sheet_skid_num A sheet_skid_detail_prod_item_num A " )
