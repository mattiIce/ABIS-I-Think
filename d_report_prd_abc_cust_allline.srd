 retrieve="  SELECT ~"AB_JOB~".~"AB_JOB_NUM~",   
         ~"CUSTOMER_ORDER~".~"ORIG_CUSTOMER_ID~",   
         ~"CUSTOMER_ORDER~".~"ENDUSER_ID~",   
         ~"ORDER_ITEM~".~"ALLOY2~",   
         ~"ORDER_ITEM~".~"TEMPER~",   
         ~"ORDER_ITEM~".~"GAUGE~",   
         ~"ORDER_ITEM~".~"ITEM_DUE_DATE~",   
         ~"AB_JOB~".~"LINE_NUM~",
			~"AB_JOB~".~"JOB_STATUS~",
			NULL ~"SPEC_T~",
			0 ~"BAL_DUE~"
    FROM ~"AB_JOB~",   
         ~"CUSTOMER_ORDER~",   
         ~"ORDER_ITEM~"  
   WHERE ( ~"AB_JOB~".~"ORDER_ABC_NUM~" = ~"CUSTOMER_ORDER~".~"ORDER_ABC_NUM~" ) and  
         ( ~"AB_JOB~".~"ORDER_ITEM_NUM~" = ~"ORDER_ITEM~".~"ORDER_ITEM_NUM~" ) and  
         ( ~"AB_JOB~".~"ORDER_ABC_NUM~" = ~"ORDER_ITEM~".~"ORDER_ABC_NUM~" ) and
			( ~"AB_JOB~".~"JOB_STATUS~" <> 2 )

"  sort="ab_job_line_num A customer_order_orig_customer_id A ab_job_ab_job_num A " )
