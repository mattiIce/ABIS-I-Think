 column=(type=decimal(0) updatewhereclause=yes name=shift_schedule_schedule_type dbname="SHIFT_SCHEDULE.SCHEDULE_TYPE" values="1ST	1/2ND	2/3RD	3/4TH	4/" )
 column=(type=decimal(0) updatewhereclause=yes name=shift_schedule_supervisor_id dbname="SHIFT_SCHEDULE.SUPERVISOR_ID" )
 column=(type=datetime updatewhereclause=yes name=shift_schedule_shift_starting_time dbname="SHIFT_SCHEDULE.SHIFT_STARTING_TIME" )
 column=(type=datetime updatewhereclause=yes name=shift_schedule_shift_ending_time dbname="SHIFT_SCHEDULE.SHIFT_ENDING_TIME" )
 column=(type=decimal(0) updatewhereclause=yes name=shift_schedule_shift_cancelled dbname="SHIFT_SCHEDULE.SHIFT_CANCELLED" values="Cancelled	1/On	0/" )
 retrieve="  SELECT SHIFT_SCHEDULE.SHIFT_SCHEDULE_DATE,   
         SHIFT_SCHEDULE.LINE_NUM,   
         SHIFT_SCHEDULE.SCHEDULE_TYPE,   
         SHIFT_SCHEDULE.SUPERVISOR_ID,   
         SHIFT_SCHEDULE.SHIFT_STARTING_TIME,   
         SHIFT_SCHEDULE.SHIFT_ENDING_TIME,   
         SHIFT_SCHEDULE.SHIFT_CANCELLED  
    FROM SHIFT_SCHEDULE   
   WHERE 
         ( ( To_Date(SHIFT_SCHEDULE.SHIFT_SCHEDULE_DATE) = :ad_date ) AND  
         ( SHIFT_SCHEDULE.LINE_NUM = :ai_line ) AND  
         ( SHIFT_SCHEDULE.SCHEDULE_TYPE = :ai_type ) )    
" arguments=(("ad_date", date),("ai_line", number),("ai_type", number)) )
