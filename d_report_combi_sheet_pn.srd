 retrieve="  SELECT ~"SHEET_PACKING_ITEM~".~"SH_PACKING_ITEM~",   
         ~"SHEET_PACKING_ITEM~".~"SHEET_PACKAGING_TICKET~",   
         ~"COIL~".~"LOT_NUM~",   
         ~"COIL~".~"COIL_ORG_NUM~",   
         ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_PIECES~",   
         case 
           (
             select distinct shipment.customer_id
             from   sheet_packing_item
                    join shipment on shipment.packing_list = sheet_packing_item.packing_list
             where  sheet_packing_item.packing_list = :al_shipment           
           )
         when 2802 then
           production_sheet_item.prod_item_theoretical_wt
         else
           production_sheet_item.prod_item_net_wt
         end prod_item_net_wt,   
         ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_THEORETICAL_WT~",   
         ~"SHEET_SKID~".~"SHEET_TARE_WT~",   
         ~"SHEET_SKID~".~"SHEET_NET_WT~",   
         ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_STATUS~",   
         ~"SHEET_SKID~".~"SHEET_SKID_DISPLAY_NUM~",
			(
            select distinct shipment.customer_id
            from   sheet_packing_item
                   join shipment on shipment.packing_list = sheet_packing_item.packing_list
            where  sheet_packing_item.packing_list = :al_shipment
         ) customer_id,
			(select nvl(package_num, 0) from dbo.sheet_skid_package where sheet_skid_num = sheet_skid.sheet_skid_num) package_num /* Alex Gerlants. 06/15/2018. Arconic_Package_Num */  
    FROM ~"PRODUCTION_SHEET_ITEM~",   
         ~"SHEET_PACKING_ITEM~",   
         ~"SHEET_SKID~",   
         ~"SHEET_SKID_DETAIL~",   
         ~"COIL~"  
   WHERE ( ~"SHEET_SKID~".~"SHEET_SKID_NUM~" = ~"SHEET_PACKING_ITEM~".~"SHEET_SKID_NUM~" ) and  
         ( ~"SHEET_SKID_DETAIL~".~"SHEET_SKID_NUM~" = ~"SHEET_SKID~".~"SHEET_SKID_NUM~" ) and  
         ( ~"SHEET_SKID_DETAIL~".~"PROD_ITEM_NUM~" = ~"PRODUCTION_SHEET_ITEM~".~"PROD_ITEM_NUM~" ) and  
         ( ~"PRODUCTION_SHEET_ITEM~".~"COIL_ABC_NUM~" = ~"COIL~".~"COIL_ABC_NUM~" ) and  
         ( ( ~"SHEET_PACKING_ITEM~".~"PACKING_LIST~" = :al_shipment ) )    
" arguments=(("al_shipment", number))  sort="sheet_packing_item_sh_packing_item A " )
