 retrieve="  SELECT ~"COIL~".~"COIL_ABC_NUM~",   
         ~"COIL~".~"CUSTOMER_ID~",   
         ~"COIL~".~"COIL_ORG_NUM~",   
         ~"COIL~".~"LOT_NUM~",   
         ~"COIL~".~"COIL_STATUS~",   
         ~"COIL~".~"COIL_TEMPER~",   
         ~"COIL~".~"COIL_GAUGE~",   
         ~"COIL~".~"NET_WT_BALANCE~",   
         ~"COIL~".~"COIL_NOTES~"  
    FROM ~"COIL~"  
   WHERE ( ~"COIL~".~"COIL_STATUS~" = 3 ) OR   /* Rejected */
         ( ~"COIL~".~"COIL_STATUS~" = 6 ) or   /* Return */
         ( ~"COIL~".~"COIL_STATUS~" = 14 )     /* Alex Gerlants. novelis testing. This is ~"Scrap~" coil status */    
" update="COIL" updatewhere=0 updatekeyinplace=no )
