event ue_openfile;call super::ue_openfile;integer li_FileNum, li_rc
String ls_line
string ls_docname, ls_named

SetPointer(HourGlass!)

li_rc = GetFileOpenName("Select File", ls_docname, ls_named, "DOC",  &
	+ "QUOTE Files (*.QOT),*.QOT," )

IF li_rc = 1 THEN 
	li_FileNum = FileOpen(ls_docname, LineMode!, Read!, LockRead!)
ELSE
	RETURN 
END IF

DO  
	li_rc = FileRead(li_FileNum, ls_line)
   IF Match(ls_line,"^D@ = ") THEN em_d$.Text = wf_trim_value(ls_line,5) 
   IF Match(ls_line,"^N@ = ") THEN sle_n$.Text = wf_trim_value(ls_line,5) 
   IF Match(ls_line,"^C@ = ") THEN sle_c$.Text = wf_trim_value(ls_line,5) 
   IF Match(ls_line,"^M@ = ") THEN sle_m$.Text = wf_trim_value(ls_line,5) 
   IF Match(ls_line,"^P@ = ") THEN sle_p$.Text = wf_trim_value(ls_line,5) 

	IF Match(ls_line, "^A = " ) THEN tab_quote.tabpage_coil.em_a.Text = wf_trim_value(ls_line,4) 
	IF Match(ls_line, "^B = " ) THEN tab_quote.tabpage_coil.em_b.Text = wf_trim_value(ls_line,4) 
	IF Match(ls_line, "^C = " ) THEN tab_quote.tabpage_coil.em_c.Text = wf_trim_value(ls_line,4) 
	IF Match(ls_line, "^D = " ) THEN tab_quote.tabpage_coil.em_d.Text = wf_trim_value(ls_line,4) 
	IF Match(ls_line, "^E1 = " ) THEN tab_quote.tabpage_coil.em_e1.Text = wf_trim_value(ls_line,5) 
	IF Match(ls_line, "^P = " ) THEN tab_quote.tabpage_coil.em_p.Text = wf_trim_value(ls_line,4) 

	IF Match(ls_line, "^TW = " ) THEN tab_quote.tabpage_pallet.em_tw.Text = wf_trim_value(ls_line,5) 
	IF Match(ls_line, "^NS1 = " ) THEN tab_quote.tabpage_pallet.em_ns1.Text = wf_trim_value(ls_line,6) 
	IF Match(ls_line, "^NW = " ) THEN tab_quote.tabpage_pallet.em_nw.Text = wf_trim_value(ls_line,5) 

LOOP UNTIL li_rc < 0

FileClose(li_FileNum)
end event

