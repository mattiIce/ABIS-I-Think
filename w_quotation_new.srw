public function integer wf_coil_info ();// line 10 to 592

Int li_rc

a = real(tab_quote.tabpage_coil.em_a.Text)
IF a < 0.001 THEN
	MessageBox("Error", "Invalid gage value (<0.01)", StopSign! )
	Return -1
END IF

b = real(tab_quote.tabpage_coil.em_b.Text)
IF b < 0.04 THEN
	MessageBox("Error", "Invalid coil width value (<0.04)", StopSign! )
	Return -1
END IF
IF b > 110 THEN
	MessageBox("Error", "Invalid coil width value (>110):: too wide", StopSign! )
	Return -1
END IF

c = real(tab_quote.tabpage_coil.em_c.Text)
IF c < 0.2 THEN
	MessageBox("Error", "Invalid feed length value (<0.2)", StopSign! )
	Return -1
END IF

d = real(tab_quote.tabpage_coil.em_d.Text)
IF d < 0.05 THEN
	li_rc = MessageBox("Info", "Invalid coil density value (<0.05), density will be reset back to 0.098, Continue?", Question!,YesNo!, 2 )
	IF li_rc = 2 THEN Return -1
	d = 0.098
END IF
IF d > 0.3 THEN
	MessageBox("Error", "Invalid coil density value (>0.3):: too high", StopSign! )
	Return -1
END IF

x1 = a*b*c*d
IF x1 = 0 THEN
	MessageBox("Error", "Invalid piece weight, please double check all values!", StopSign! )
	Return -1
ELSE
	tab_quote.tabpage_coil.em_x1.Text = String(x1, "###,###.##" )
END IF	

IF d <= 0.105 THEN
	e = 160*b
	IF b <= 36 THEN 
		n = 550*b
	ELSE
		n = 370*b
	END IF
ELSE
	e = 270*b
	IF b <= 36 THEN n = 1000*b
	IF b > 36 THEN n = 900*b
	IF b >60 THEN n = 1000*b
END IF
IF n > 30000 THEN n = 30000
tab_quote.tabpage_coil.em_e.Text = String(e, "#,###,###.##" )
tab_quote.tabpage_coil.em_n.Text = String(n, "#,###,###.##" )
e = real(tab_quote.tabpage_coil.em_e1.Text)
IF e>N THEN
	MessageBox("Error", "Invalid coil weight,(> maxinum coil weight) ",StopSign! )
	Return -1
END IF

p = real(tab_quote.tabpage_coil.em_p.Text)
IF p < 1 THEN
	MessageBox("Error", "Invalid total material weight!",StopSign! )
	Return -1
END IF
IF p <0.5*e THEN
	MessageBox("Info","Consider a lot charge!") 
ELSE
	IF e > p THEN 
		e = p
		tab_quote.tabpage_coil.em_e.Text = String(e, "#,###,###.##" )
	END IF
	IF e < 100*b THEN MessageBox("Info","Consider a lot charge!")
END IF

//markup
hi = real(tab_quote.tabpage_process.em_hi.Text)
IF hi > 0 AND hi < 100 THEN
	hi = (100 - hi) / 100
ELSE
	MessageBox("Info", "Invalid markup value, system will reset it back to 30%")
	hi = 0.7
END IF

u = Int(p/e + 0.5)
e = Int(p/u)
tab_quote.tabpage_coil.em_e1.Text = String(e, "#,###,###.##" )
tab_quote.tabpage_coil.em_u.Text = String(u, "#,###,###" )

od = Ceiling( 2 * sqrt(e/(3.141592*b*d) + 100) )
tab_quote.tabpage_coil.em_od.Text = String(od, "#,###,###" )

IF tab_quote.tabpage_coil.cbx_pt$.Checked THEN
	CHOOSE CASE b
		CASE IS < 24.00001 
			pt =1 
		CASE 24.00001 TO 48 
			pt = 2
		CASE 48.00001 TO 75
			pt = 3
		CASE ELSE 
			pt = 4
	END CHOOSE
	tab_quote.tabpage_coil.em_pt.Text = String(pt, "###" )	
END IF

Return 1

end function

public function integer wf_pallet_info ();// Line 593 to 10000
IF NOT(tab_quote.tabpage_pallet.cbx_u$.Checked) THEN
	CHOOSE CASE a
		CASE IS < 0.125
			ab = Int(c/30) + 2
		CASE 0.125 TO 0.1879999
			ab = Int(c/42) + 2
		CASE 0.188 TO 0.2499999
			ab = Int(c/60) + 2
		CASE ELSE
			ab = Int(c/72) + 2
	END CHOOSE
	IF ab < 3 THEN ab = 3
	ns = Integer(tab_quote.tabpage_pallet.em_ns.Text)
	IF ns < 1 THEN ns = 1
	IF  c > b THEN
		kw = b * ns
		kl = c
	ELSE
		kw = c * ns
		kl = b
	END IF
	IF wf_line_680()< 0 THEN Return -1
ELSE
	IF tab_quote.tabpage_pallet.cbx_s$.Checked THEN
		ns = 1
		nl = 1
		nw = 1
		//17000
//		IF tab_quote.tabpage_pallet.cbx_am$.Checked THEN
//			IF tab_quote.tabpage_pallet.cbx_sp$.Checked THEN
//				
//			END IF
//		ELSE
//		END IF	
		IF c > b THEN 
			kw = b
			kl = c
		ELSE
			kw = c
			kl = b
		END IF
		IF wf_line_680()< 0 THEN Return -1
	ELSE
		ns = Real(tab_quote.tabpage_pallet.em_ns1.Text)
		nw = Real(tab_quote.tabpage_pallet.em_nw.Text)
		IF nw < 1 THEN
			MessageBox("Error", "Invalid number of stacks along material width!", StopSign!)
			Return -1
		END IF
		
		IF nw < ns THEN
			nl = ns/nw
		ELSE
			IF nw = ns THEN 
				nl = 1 
			ELSEIF nw = 1 THEN 
				nl = ns
			END IF
		END IF
		IF (nw > 1) AND (nl > 1) THEN
			//4000-ok
			IF (b > c) AND (nw = nl) THEN
				kl = nw * b
				kw = nl * c
			END IF
			IF (c > b) AND (nw = nl) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b > c) AND (nl > nw) AND (nl*c <= nw * b) THEN
				kl = nw * b
				kw = nl * c
			END IF
			IF (b > c) AND (nl > nw) AND (nl*c >= nw * b) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b < c) AND (nl > nw) AND (nl*c >= nw * b) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b < c) AND (nl > nw) AND (nl*c <= nw * b) THEN
				kl = nw * b
				kw = nl * c
			END IF
			IF (b = c) AND (nl > nw)  THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b = c) AND (nl = nw)  THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b < c) AND (nl < nw) AND (nl*c >= nw * b) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (b < c) AND (nl < nw) AND (nl*c <= nw * b) THEN
				kl = nw * b
				kw = nl * c
			END IF			
		ELSE
			//935 -ok
			IF (nl =1) AND (nw > 1) AND (nw*b > nl*c) THEN
				kl = nw * b
				kw = nl * c
			END IF
			IF (nl =1) AND (nw > 1) AND (nw*b <= nl*c) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (nl > 1) AND (nw = 1) AND (nw*b <= nl*c) THEN
				kl = nl * c
				kw = nw * b
			END IF
			IF (nl > 1) AND (nw = 1) AND (nw*b > nl*c) THEN
				kl = nw * b
				kw = nl * c
			END IF
		END IF
		//965-ok
		tab_quote.tabpage_pallet.em_kl.Text = String(kl, "###,###.##")
		tab_quote.tabpage_pallet.em_kw.Text = String(Kw, "###,###.##")
		IF wf_line_680()< 0 THEN Return -1
	END IF
END IF
IF wf_line_1180() < 1 THEN Return -1
IF ab <= 1 THEN
	IF (nw = 1) OR (nl = 1) THEN
		IF wf_line_5000() < 1 THEN Return 1
	ELSE
		IF wf_line_8000() < 1 THEN Return -1
	END IF
END IF
IF wf_line_6600()< 1 THEN Return -1
//7040
tab_quote.tabpage_cost.em_j.Text = String((j / hi), "###,###.##")
tab_quote.tabpage_cost.em_jp.Text = String((jp / hi), "###,###.##")
tab_quote.tabpage_cost.em_j_jp.Text = String((jp / hi + j / hi), "###,###.##")

tab_quote.tabpage_pallet.em_kd.Text = String(kd, "###,###")
tab_quote.tabpage_pallet.em_kt.Text = String(kt, "###,###")
tab_quote.tabpage_pallet.em_kp.Text = String(kp, "###,###")
tab_quote.tabpage_pallet.em_ky.Text = String(ky, "###,###")
tab_quote.tabpage_pallet.em_kz.Text = String(kz, "###,###")
tab_quote.tabpage_pallet.em_kf.Text = String(kf, "###,###")
tab_quote.tabpage_pallet.em_ko.Text = String(ko, "###,###.####")
tab_quote.tabpage_pallet.em_ab.Text = String(ab, "###,###")

Int li_rc
li_rc = MessageBox("Question", "Would you like an alternate skid cost", Question!, YesNo!, 2)
IF li_rc = 1 THEN
	o$ = TRUE
	IF wf_alt_skid_cost() < 1 THEN Return -1
ELSE
	o$ = FALSE
END IF

Return 1

end function

public function integer wf_line_680 ();// line 680 to 880
Int li_rc
tw = Real(tab_quote.tabpage_pallet.em_tw.Text)
IF tw < 1 THEN 
	MessageBox("Error", "Invalid trial skid weight!", StopSign!)
	Return -1
END IF
IF tw > 10000 THEN
	MessageBox("Error","Invalid trail skid weight (Maxinum 10000 LBs)!", StopSign!)
	Return -1
END IF

th = ((tw/x1)/ns)*a
tab_quote.tabpage_pallet.em_th.Text = String(th, "###,###.##")
//li_rc = MessageBox("Question", "Is this trial skid WT. & HT. okay?", Question!, YesNo!, 2)
li_rc = 1 // DO NOT ask user
IF li_rc = 2 THEN Return -1
IF tw >= p THEN f = p
h = p/tw
q = Int(h + 0.8)
tw = p/q
no = Int(tw/x1 + 0.5 )
f = no*x1
kh = (no/ns) * a
tab_quote.tabpage_pallet.em_kh.Text = String(kh, "###,###.###")

Return 1
end function

public function integer wf_line_1180 ();// from line 1180 to 1290
no = Int(no / ns) * ns
kh = no * a / ns
f = no * x1
IF f * q > e * u THEN f = (e * u ) / q
IF nl = 1 THEN 
	zb = 2
ELSE
	zb = nl
END IF
IF nw = 1 THEN
	zc = 2
ELSE
	zc = nw
END IF
tab_quote.tabpage_pallet.em_q.Text = String(q, "###,###" )
tab_quote.tabpage_pallet.em_f.Text = String(f, "###,###.##" )
tab_quote.tabpage_pallet.em_no.Text = String(no, "###,###" )
tab_quote.tabpage_pallet.em_ns1.Text = String(ns, "###,###" )
tab_quote.tabpage_pallet.em_kh.Text = String(kh, "###,###.##" )
tab_quote.tabpage_pallet.em_kw.Text = String(kw, "###,###.##" )
tab_quote.tabpage_pallet.em_kl.Text = String(kl, "###,###.##" )
tab_quote.tabpage_pallet.em_zb.Text = String(zb, "###" )
tab_quote.tabpage_pallet.em_zc.Text = String(zc, "###" )

Int li_rc
//li_rc = MessageBox("Queation", "Is this skid information okay?", Question!, YesNo!, 2)
li_rc = 1   //Do NOT ask user here
IF li_rc <> 1 THEN return -1
