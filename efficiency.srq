$PBExportHeader$efficiency.srq
SELECT ((avg(SHIFT.END_TIME - SHIFT.START_TIME) - sum(DT_INSTANCE.ENDING_TIME - DT_INSTANCE.STARTING_TIME)) / (avg(SHIFT.END_TIME - SHIFT.START_TIME))) * 100
FROM DT_INSTANCE, SHIFT
WHERE DT_INSTANCE.SHIFT_NUM = SHIFT.SHIFT_NUM AND SHIFT.SHIFT_NUM=  :al_shift
GROUP BY DT_INSTANCE.SHIFT_NUM ARG(NAME = "al_shift" TYPE = number) 