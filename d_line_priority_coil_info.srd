 retrieve=" SELECT    ~"AB_JOB~".~"AB_JOB_NUM~" ,
           ~"AB_JOB~".~"JOB_STATUS~" ,
			  ~"AB_JOB~".~"LINE_NUM~",
			~"PROCESS_COIL~".~"PROCESS_QUANTITY~",   
         ~"PROCESS_COIL~".~"PROCESS_END_WT~",   
         ~"PROCESS_COIL~".~"COIL_ABC_NUM~" 
        FROM ~"AB_JOB~" ,
           ~"LINE_PRIORITY~",
				~"PROCESS_COIL~" 
   WHERE  ( ab_job.ab_job_num = line_priority.ab_job_num (+)) and  
         ( ab_job.line_num = line_priority.line_num (+)) and
			( process_coil.ab_job_num(+) = ab_job.ab_job_num) and  
         ( (~"AB_JOB~".~"JOB_STATUS~" IN (2,1)) OR (~"LINE_PRIORITY~".~"PRIORITY_NUM~"  > 0)  )   
   ORDER BY ~"AB_JOB~".~"LINE_NUM~", ~"AB_JOB~".~"AB_JOB_NUM~"    
" )
