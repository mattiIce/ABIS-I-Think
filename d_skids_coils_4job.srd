 retrieve="select (select coil.customer_id from coil where coil.coil_abc_num = production_sheet_item.coil_abc_num) coil_customer_id,
		 (select customer_short_name from customer where customer_id = (select coil.customer_id from coil where coil.coil_abc_num = production_sheet_item.coil_abc_num)) customer_short_name,
       sheet_skid.ab_job_num,
       sheet_skid.sheet_skid_num,
       sheet_skid.skid_sheet_status,
       (select coil.coil_abc_num from coil where coil.coil_abc_num = production_sheet_item.coil_abc_num) coil_abc_num,
       (select nvl(coil.coil_org_num, '') from coil where coil.coil_abc_num = production_sheet_item.coil_abc_num) coil_org_num,
		 ' ' received_863
from   production_sheet_item  
       join sheet_skid_detail on sheet_skid_detail.prod_item_num = production_sheet_item.prod_item_num
       join sheet_skid on sheet_skid.sheet_skid_num = sheet_skid_detail.sheet_skid_num
where  sheet_skid.ab_job_num = :al_ab_job_num
and    sheet_skid.skid_sheet_status = 2 /* Ready */
order by
       (select coil.coil_abc_num from coil where coil.coil_abc_num = production_sheet_item.coil_abc_num) asc" arguments=(("al_ab_job_num", number)) )
