 retrieve="  SELECT ~"SHEET_PACKING_ITEM~".~"SH_PACKING_ITEM~",   
         ~"SHEET_PACKING_ITEM~".~"SHEET_SKID_NUM~",   
         ~"SHEET_PACKING_ITEM~".~"SHEET_PACKAGING_TICKET~",   
         ~"SHEET_PACKING_ITEM~".~"PACKING_LIST~",   
         ~"SHEET_SKID~".~"SKID_PIECES~",   
         case 
           (
              select distinct shipment.customer_id
              from   sheet_packing_item
                     join shipment on shipment.packing_list = sheet_packing_item.packing_list
              where  sheet_packing_item.packing_list = :cur_pack_list
           )
         when 2802 then
           sheet_skid.sheet_theoretical_wt
         else
           sheet_skid.sheet_net_wt
         end sheet_skid_sheet_net_wt,
         ~"SHEET_SKID~".~"SHEET_TARE_WT~",   
         ~"SHEET_SKID~".~"SKID_SHEET_STATUS~",
			'               ' as notes  
    FROM ~"SHEET_PACKING_ITEM~",   
         ~"SHEET_SKID~"  
   WHERE ( ~"SHEET_PACKING_ITEM~".~"SHEET_SKID_NUM~" = ~"SHEET_SKID~".~"SHEET_SKID_NUM~" ) and  
         ( ( ~"SHEET_PACKING_ITEM~".~"PACKING_LIST~" = :cur_pack_list ) AND  
         ( ~"SHEET_SKID~".~"SKID_FROM_IF_WHED~" is NULL ) )" update="SHEET_PACKING_ITEM" updatewhere=1 updatekeyinplace=yes arguments=(("cur_pack_list", number))  sort="sh_packing_item A " )
