from coil c, ab_job j, process_coil p, customer_order o, order_item i, customer cu1, customer cu2, recovery_job_coil rjc, product_type pt
where (j.order_item_num = i.order_item_num and j.order_abc_num = i.order_abc_num) 
and i.order_abc_num = o.order_abc_num 
and o.orig_customer_id = cu1.customer_id 
and o.enduser_id = cu2.customer_id 
and c.coil_abc_num = p.coil_abc_num 
and j.ab_job_num = p.ab_job_num 
and p.coil_abc_num = rjc.coil_abc_num
and p.ab_job_num = rjc.ab_job_num
and j.job_done_time >= :ad_start
and j.job_done_time <= :ad_end
and o.orig_customer_id = :al_cust
and rjc.product_type_id = pt.product_type_id
" arguments=(("ad_start", datetime),("ad_end", datetime),("al_cust", number)) )
