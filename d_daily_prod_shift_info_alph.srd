 column=(type=decimal(0) updatewhereclause=yes name=schedule_type dbname="SHIFT.SCHEDULE_TYPE" )
 retrieve="  SELECT SHIFT.SHIFT_NUM,   
         SHIFT.LINE_NUM,   
         SHIFT.START_TIME,   
         SHIFT.END_TIME,   
         SHIFT.SCHEDULE_TYPE  
    FROM SHIFT  
   WHERE ( SHIFT.LINE_NUM = :al_line ) AND  
         (To_date(SHIFT.START_TIME) >= :ad_s) AND
         (To_date(SHIFT.START_TIME) <= :ad_e)    
   
" update="SHIFT" updatewhere=0 updatekeyinplace=no arguments=(("al_line", number),("ad_s", date),("ad_e", date)) )
